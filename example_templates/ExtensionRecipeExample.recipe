<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Description</key>
        <string>Downloads Adobe Shockwave Player installer and extracts the embedded package, uploads to JSS.</string>
        <key>Identifier</key>
        <string>com.github.novaksam.AdobeShockwavePlayer.jss</string>
        <key>Input</key>
        <dict>
            <key>NAME</key>
            <string>Adobe Shockwave Player</string>
            <key>CATEGORY</key>
            <string>Plugins and Frameworks</string>
            <key>POLICY_CATEGORY</key>
            <string>Third Party Applications</string>
            <key>POLICY_TEMPLATE</key>
            <string>%RECIPE_DIR%/PolicyTemplate.xml</string>
            <key>GROUP_NAME</key>
            <string>Outdated Shockwave - Test</string>
            <key>GROUP_TEMPLATE</key>
            <string>%RECIPE_DIR%/SmartGroupTemplate-Extension.xml</string>
            <key>ICON</key>
            <string>%RECIPE_DIR%/Shockwave.png</string>
            <key>EXTENSION_NAME</key>
            <string>Shockwave Version - test</string>
            <key>EXTENSION_PATH</key>
            <string>/Library/Internet\ Plug-Ins/DirectorShockwave.plugin/Contents</string>
            <key>EXTENSION_PARAMETER</key>
            <string>CFBundleShortVersionString</string>
            <key>EXTENSION_DATA_TYPE</key>
            <string>string</string>
            <key>EXTENSION_TEMPLATE</key>
            <string>%RECIPE_DIR%/ExtensionAttributeTemplate.xml</string>
            <key>EXTENSION_DESCRIPTION</key>
            <string>Returns the installed version of Adobe Shockwave.</string>
        </dict>
        <key>MinimumVersion</key>
        <string>0.4.0</string>
        <key>ParentRecipe</key>
        <string>com.github.jleggat.AdobeShockwavePlayer.pkg</string>
        <key>Process</key>
        <array>
            <dict>
                <key>Arguments</key>
                <dict>
                    <key>prod_name</key>
                    <string>%NAME%</string>
                    <key>category</key>
                    <string>%CATEGORY%</string>
                    <key>policy_category</key>
                    <string>%POLICY_CATEGORY%</string>
                    <key>policy_template</key>
                    <string>%POLICY_TEMPLATE%</string>
                    <key>self_service_icon</key>
                    <string>%ICON%</string>
                    <key>extension_name</key>
                    <string>%EXTENSION_NAME%</string>
                    <key>extension_path</key>
                    <string>%EXTENSION_PATH%</string>
                    <key>extension_parameter</key>
                    <string>%EXTENSION_PARAMETER%</string>
                    <key>extension_data_type</key>
                    <string>%EXTENSION_DATA_TYPE%</string>
                    <key>extension_description</key>
                    <string>%EXTENSION_DESCRIPTION%</string>
                    <key>groups</key>
                    <array>
                        <dict>
                            <key>name</key>
                            <string>%GROUP_NAME%</string>
                            <key>smart</key>
                            <true/>
                            <key>template_path</key>
                            <string>%GROUP_TEMPLATE%</string>
                        </dict>
                    </array>
                    <key>extension_attributes</key>
                    <array>
                        <dict>
                            <key>extension_name</key>
                            <string>%EXTENSION_NAME%</string>
                            <key>extension_template</key>
                            <string>%EXTENSION_TEMPLATE%</string>
                        </dict>
                    </array>
                </dict>
                <key>Processor</key>
                <string>JSSImporter</string>
            </dict>
        </array>
    </dict>
</plist>
